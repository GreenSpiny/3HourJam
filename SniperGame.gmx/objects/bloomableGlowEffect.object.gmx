        var tempAlpha = trailAlpha;     
        var tScaleFactor = 1;            
        for ( var i = ds_list_size( positionList ) - 1; i &gt;= 0 ; i -= NUM_VALUES ) {
            draw_sprite_ext( image, image_index, 
