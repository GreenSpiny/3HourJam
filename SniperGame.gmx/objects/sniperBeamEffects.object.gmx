var EFFECT_RESOLUTION = 16;
var EFFECT_AURA_WIDTH = 8;
var EFFECT_OUTER_WIDTH = 32;
var EFFECT_CORE_WIDTH = 2;
var EFFECT_CONE_ANGLE = 135;


var tStartX = x + lengthdir_x( fireOffset, angle );
var tStartY = y + lengthdir_y( fireOffset, angle );
var tEndX = x;
var tEndY = y
for ( var i = 0; i &lt; collPointsSize; i += 2 ) {
    tEndX = collPoints[ i ];//ds_list_find_value( collPointList, i );
    tEndY = collPoints[ i + 1 ];//ds_list_find_value( collPointList, i + 1 );
    
    var tDist = point_distance( tStartX, tStartY, tEndX, tEndY );
    var tAngle = point_direction( tStartX, tStartY, tEndX, tEndY );
    
    for ( var j = 0; j &lt; tDist; j += EFFECT_RESOLUTION ) {
        part_type_direction( getParticle( "stingerLaser1" ), tAngle + EFFECT_CONE_ANGLE, tAngle + 180, 0, 0 );
        part_particles_create( system1, 
                                tStartX + lengthdir_x( j, tAngle ) + lengthdir_x( random( EFFECT_AURA_WIDTH ), tAngle + 90 ),
                                tStartY + lengthdir_y( j, tAngle ) + lengthdir_y( random( EFFECT_AURA_WIDTH ), tAngle + 90 ),
                                getParticle( "stingerLaser1" ), 1 );
        part_type_direction( getParticle( "stingerLaser1" ), tAngle - 180, tAngle - EFFECT_CONE_ANGLE, 0, 0 );
        part_particles_create( system1, 
                                tStartX + lengthdir_x( j, tAngle ) + lengthdir_x( random( EFFECT_AURA_WIDTH ), tAngle - 90 ),
                                tStartY + lengthdir_y( j, tAngle ) + lengthdir_y( random( EFFECT_AURA_WIDTH ), tAngle - 90 ),
                                getParticle( "stingerLaser1" ), 1 );
        
        part_type_direction( getParticle( "stingerLaser2" ), tAngle, tAngle, 0, 0 );                                               
        part_particles_create( system1, 
                                tStartX + lengthdir_x( j, tAngle ) + lengthdir_x( random( EFFECT_CORE_WIDTH ), tAngle + 90 ),
                                tStartY + lengthdir_y( j, tAngle ) + lengthdir_y( random( EFFECT_CORE_WIDTH ), tAngle + 90 ),
                                getParticle( "stingerLaser2" ), 1 );
        part_particles_create( system1, 
                                tStartX + lengthdir_x( j, tAngle ) + lengthdir_x( random( EFFECT_CORE_WIDTH ), tAngle - 90 ),
                                tStartY + lengthdir_y( j, tAngle ) + lengthdir_y( random( EFFECT_CORE_WIDTH ), tAngle - 90 ),
                                getParticle( "stingerLaser2" ), 1 );
                                
        part_type_direction( getParticle( "stingerLaser3" ), tAngle + EFFECT_CONE_ANGLE, tAngle + 180, 0, 0 );
        part_particles_create( system1, 
                                tStartX + lengthdir_x( j, tAngle ) + lengthdir_x( random( EFFECT_AURA_WIDTH ), tAngle + 90 ),
                                tStartY + lengthdir_y( j, tAngle ) + lengthdir_y( random( EFFECT_AURA_WIDTH ), tAngle + 90 ),
                                getParticle( "stingerLaser3" ), 1 );
        part_type_direction( getParticle( "stingerLaser3" ), tAngle - 180, tAngle - EFFECT_CONE_ANGLE, 0, 0 );
        part_particles_create( system1, 
                                tStartX + lengthdir_x( j, tAngle ) + lengthdir_x( random( EFFECT_AURA_WIDTH ), tAngle - 90 ),
                                tStartY + lengthdir_y( j, tAngle ) + lengthdir_y( random( EFFECT_AURA_WIDTH ), tAngle - 90 ),
                                getParticle( "stingerLaser3" ), 1 );
                                
        part_type_direction( getParticle( "stingerLaser4" ), tAngle, tAngle, 0, 0 );                                               
        part_particles_create( system1, 
                                tStartX + lengthdir_x( j, tAngle ) + lengthdir_x( random( EFFECT_OUTER_WIDTH ), tAngle + 90 ),
                                tStartY + lengthdir_y( j, tAngle ) + lengthdir_y( random( EFFECT_OUTER_WIDTH ), tAngle + 90 ),
                                getParticle( "stingerLaser4" ), 1 );
        part_particles_create( system1, 
                                tStartX + lengthdir_x( j, tAngle ) + lengthdir_x( random( EFFECT_OUTER_WIDTH ), tAngle - 90 ),
                                tStartY + lengthdir_y( j, tAngle ) + lengthdir_y( random( EFFECT_OUTER_WIDTH ), tAngle - 90 ),
                                getParticle( "stingerLaser4" ), 1 );
    }
    
    tStartX = tEndX;
    tStartY = tEndY;
    
}


